  "description": "A feature detection library",
  "engines": {
    "npm": ">=3.0.0"
  },
  "homepage": "https://dojo.io",
  "bugs": {
    "url": "https://github.com/dojo/has/issues"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "main": "main.js",
  "repository": {
